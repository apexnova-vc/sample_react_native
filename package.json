{
    "name": "rn_blueprint",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "./node_modules/react-native/cli.js run-android",
        "ios": "./node_modules/react-native/cli.js run-ios",
        "lint": "./node_modules/eslint/bin/eslint.js .",
        "start": "./node_modules/react-native/cli.js start",
        "test": "./node_modules/jest/bin/jest.js"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-native": "0.72.3",
        "react-native-gradle-plugin": "0.71.19"
    },
    "devDependencies": {
        "@babel/core": "7.22.1",
        "@typescript-eslint/eslint-plugin": "6.3.0",
        "eslint": "8.46.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-react": "7.33.1",
        "eslint-plugin-react-hooks": "4.6.0",
        "jest": "29.5.0",
        "metro-react-native-babel-preset": "0.77.0",
        "prettier": "3.0.0",
        "react-native-flipper": "0.200.0",
        "react-test-renderer": "18.2.0",
        "typescript": "5.1.6"
    },
    "jest": {
        "collectCoverageFrom": [
            "app//*.{js,jsx,ts,tsx}",
            "!/.d.ts"
        ],
        "testMatch": [
            "<rootDir>/app/**/?(.)(spec|test).(j|t)s?(x)",
            "<rootDir>/app/**/__tests__/**/*.(j|t)s?(x)"
        ],
        "testEnvironment": "node",
        "modulePathIgnorePatterns": [
            "dist/"
        ],
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ]
    }
}